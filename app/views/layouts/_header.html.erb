<div id="header">
  <div id="logo">
    <%= link_to image_tag("logo.jpg", height: "50px", width: "50px"), characters_path %>
  </div>
  <div id="navigation">
    <p>
      <% if current_user %>
        <%= current_user.name %>
        <span class="separator">|</span>
        <%= link_to 'Log Out', logout_path  %>
      <% else %>
      Sign Up
      <%#= link_to 'Sign Up', signup_path  %>
      <span class="separator">|</span>
      Log In
      <%#= link_to 'Log In', login_path  %>
      <% end %>
    </p>
  </div>
  <span id="title">
    A Wheel of Time Personography
  </span>
</div>
